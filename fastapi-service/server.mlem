type: fastapi
middlewares:
  - type: prometheus_fastapi
    metrics:
      - "lang_metric.random_value"
      - "lang_metric.http_requested_languages_total"
      - "lang_metric.image_middle_pixel_value"
      - "monitoring.request_size"
